class Solution{
	void toBinary(int N) {
	    
	   String temp = "";
	   
	   while(N>0)
	   {
	       int rem = N%2;
	       temp += rem;
	       N = N/2;
	       
	   }
	    
	   StringBuilder sb = new StringBuilder(temp);
	   
	   String binary = sb.reverse().toString();
	   
	   System.out.print(binary);
	    
	}

}
